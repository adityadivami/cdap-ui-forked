{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": false,
    "object-literal-key-quotes": [
      false
    ],
    "member-ordering": false,
    "semicolon": false,
    "ordered-imports": [false],
    "quotemark": [false]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "ui-utils",
      "directives",
      "services",
      "tracker",
      "styles",
      "logviewer",
      "hydrator",
      "lib",
      "common",
      "node_modules",
      "__mocks__"
    ]
  }
}